{
  "name": "react-simple-todo",
  "version": "1.0.0",
  "description": "A very simple React TODO app with Typescrip and Redux",
  "main": "index.js",
  "scripts": {
    "build": "yarn lint && yarn test && webpack",
    "test": "yarn lint && yarn jest",
    "test:watch": "jest --watchAll",
    "lint": "standard",
    "start": "concurrently \"yarn start:server\" \"yarn start:app\"",
    "start:app": "webpack-dev-server",
    "start:server": "json-server --watch db.json --port 3001 --quiet"
  },
  "keywords": [
    "React",
    "Typescript",
    "Redux",
    "TODO",
    "SASS",
    "Webpack"
  ],
  "author": "th3N0m4d",
  "license": "ISC",
  "babel": {
    "presets": [
      "@babel/env",
      "@babel/react"
    ],
    "plugins": [
      "@babel/plugin-proposal-class-properties"
    ]
  },
  "standard": {
    "parser": "babel-eslint",
    "env": [
      "jest"
    ],
    "ignore": [
      "dist/**"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn test"
    }
  },
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/preset-env": "^7.9.0",
    "@babel/preset-react": "^7.9.4",
    "@types/jest": "^25.1.4",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^25.2.4",
    "babel-loader": "^8.1.0",
    "clean-webpack-plugin": "^3.0.0",
    "concurrently": "^5.1.0",
    "css-loader": "^3.4.2",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "file-loader": "^6.0.0",
    "html-webpack-plugin": "^4.0.3",
    "husky": "^4.2.3",
    "jest": "^25.2.4",
    "jest-enzyme": "^7.1.2",
    "node-sass": "^4.13.1",
    "redux-mock-store": "^1.5.4",
    "sass-loader": "^8.0.2",
    "standard": "^14.3.3",
    "style-loader": "^1.1.3",
    "webpack": "^4.42.1",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.10.3"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.28",
    "@fortawesome/free-solid-svg-icons": "^5.13.0",
    "@fortawesome/react-fontawesome": "^0.1.9",
    "axios": "^0.19.2",
    "bootstrap": "^4.4.1",
    "classnames": "^2.2.6",
    "json-server": "^0.16.1",
    "prop-types": "^15.7.2",
    "ramda": "^0.27.0",
    "react": "^16.13.1",
    "react-bootstrap": "^1.0.0",
    "react-dom": "^16.13.1",
    "react-redux": "^7.2.0",
    "redux": "^4.0.5",
    "redux-thunk": "^2.3.0",
    "uuid": "^7.0.3"
  }
}